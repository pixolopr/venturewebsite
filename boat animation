            /*boatdiv
startupsigndiv
islanddiv*/
        $('.startupsigndiv').hide();
        $('.slidertagdiv').hide();

        var animationEnd = 'webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend';

        var slider2animate = function () {

            var boatanimaticomp1 = function () {
                setTimeout(slider3animate, 3000);
            };
            var slidertag2 = function () {

                $('.boatdiv').animate({
                    top: '15%',
                    left: '23%',
                    width: '20%'
                }, 5000, boatanimaticomp1);

                $('.slidertagdiv').show();
                $('.slidertagdiv').addClass('bounceIn').one(animationEnd, function () {
                    $('.slidertagdiv').removeClass('bounceIn');
                });


            };

            $('.startupsigndiv').addClass('fadeOut').one(animationEnd, function () {
                $('.startupsigndiv').removeClass('animated fadeOut');
                $('.startupsigndiv').hide();
            });

            $('.slidertagdiv').addClass('bounceOut').one(animationEnd, function () {
                $('.slidertagdiv').removeClass('bounceOut');
                $('.slidertagdiv').hide();
                $('#headtag').text('We can help ypu');
                $('#paratag').text('we will help you find the island of success');
                setTimeout(slidertag2, 1000);
            });







        };

        var slider3animate = function () {

            var slider1call = function () {
                $('.slidertagdiv').addClass('bounceOut').one(animationEnd, function () {
                    $('.slidertagdiv').removeClass('bounceOut');
                    $('.slidertagdiv').hide();
                    $('#headtag').text('Is your startup lost in the Ocean');
                    $('#paratag').text('Have an amzing idea and dont know what to do with it ?');
                    slider1animate();
                });




            };
            var boatanimaticomp2 = function () {
                setTimeout(slider1call, 3000);
            };

            var slidertag3 = function () {
                $('.slidertagdiv').show();
                $('.slidertagdiv').addClass('bounceIn').one(animationEnd, function () {
                    $('.slidertagdiv').removeClass('bounceIn');
                });
            };

            $('.slidertagdiv').addClass('bounceOut').one(animationEnd, function () {
                $('.slidertagdiv').removeClass('bounceOut');
                $('.slidertagdiv').hide();
                $('#headtag').text('Will be by your side till you steer yourself to success island');
                $('#paratag').text('Thats where the party is');
                slidertag3();
            });



            $('.boatdiv').animate({
                top: '25%',
                left: '43%',
                width: '32%'
            }, 5000, boatanimaticomp2);



            $('.islanddiv').animate({
                left: '65%'
            }, 5000);
        };

        var slider1animate = function () {

            $('.boatdiv').css({
                top: '15%',
                left: '8%',
                width: '14%'
            });
            $('.islanddiv').css({
                left: '105%'
            });
            $('.slidertagdiv').addClass('animated');

            $('.boatdiv').addClass('animated fadeIn').one(animationEnd, function () {
                $('.boatdiv').removeClass('animated fadeIn');

                $('.startupsigndiv').show();
                $('.startupsigndiv').addClass('animated slideInUp').one(animationEnd, function () {
                    $('.startupsigndiv').removeClass('slideInUp');

                    $('.slidertagdiv').show();
                    $('.slidertagdiv').addClass('bounceIn').one(animationEnd, function () {
                        $('.slidertagdiv').removeClass('bounceIn');
                        setTimeout(slider2animate, 3000);
                    });


                });
            });
        };
        slider1animate();
        
        
        
    .islanddiv {
    width: 30%;
    position: absolute;
    left: 105%;
    z-index: 1;
}

.wave {
    height: 300px;
    position: absolute;
    width: 100%;
}

.wave4 {
    .wave;
    margin-top: 1%;
    background-repeat: no-repeat;
    background-image: url(../images/wave_4.png);
    background-size: 110%;
    background-position-x: 0%; //-> 70%
    -webkit-animation: wave4move 5s ease-out infinite alternate-reverse;
    ;
    /* Chrome, Safari, Opera */
    animation: wave4move 5s ease-out infinite alternate-reverse;
}

